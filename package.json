{
  "name": "maplibre-gl-svg",
  "version": "0.0.1",
  "description": "A maplibre-gl-js plugin that adds support for loading SVGs and SVG templates as images into the maps image sprite.",
  "keywords": [
    "maplibre",
    "maps",
    "maplibre-gl-js",
    "svg",
    "image",
    "image-pattern"
  ],
  "bugs": {
    "url": "https://github.com/mosaic-ground-control/maplibre-gl-svg/issues"
  },
  "homepage": "https://github.com/mosaic-ground-control/maplibre-gl-svg#readme",
  "scripts": {
    "build": "tsc && vite build"
  },
  "author": "Ricky Brundritt",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^10.0.10",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.19",
    "maplibre-gl": "^5.1.0",
    "tslint": "^6.1.3",
    "typescript": "^5.3.2"
  },
  "types": "dist/index.d.ts",
  "files": [
    "typings/**/*",
    "dist/**/*",
    "license.md"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/maplibre-gl-svg.es.js",
        "types": "./dist/index.d.ts"
      },
      "require": "./dist/maplibre-gl-svg.umd.js"
    }
  },
  "dependencies": {
    "vite": "^6.1.0"
  }
}
